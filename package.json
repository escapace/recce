{
  "name": "recce",
  "version": "1.3.1",
  "author": "escapace <opensource@escapace.com>",
  "description": "TypeScript library build tool",
  "license": "MPL-2.0",
  "bin": {
    "recce": "./bin/run"
  },
  "bugs": "https://github.com/escapace/recce/issues",
  "dependencies": {
    "@babel/code-frame": "7.0.0-beta.51",
    "@babel/core": "7.0.0-beta.51",
    "@babel/plugin-syntax-dynamic-import": "7.0.0-beta.51",
    "@babel/preset-env": "7.0.0-beta.51",
    "@escapace/logger": "0.0.5",
    "@oclif/command": "1.4.33",
    "@oclif/config": "1.6.33",
    "@oclif/errors": "1.1.2",
    "@oclif/plugin-help": "2.0.5",
    "babel-loader": "8.0.0-beta.2",
    "babel-plugin-lodash": "3.3.4",
    "chalk": "2.4.1",
    "tslib": "1.9.3",
    "find-npm-prefix": "1.0.2",
    "gulp": "github:gulpjs/gulp#v4.0.0-alpha.3",
    "gulp-babel": "8.0.0-beta.2",
    "gulp-filter": "5.1.0",
    "gulp-sourcemaps": "2.6.4",
    "gulp-typescript": "5.0.0-alpha.2",
    "immer": "1.3.1",
    "lodash": "4.17.10",
    "lodash-webpack-plugin": "0.11.5",
    "merge2": "1.2.2",
    "micromatch": "3.1.10",
    "node-object-hash": "1.4.1",
    "normalize-package-data": "2.4.0",
    "pkg-up": "2.0.0",
    "redux": "4.0.0",
    "reselect": "3.0.1",
    "resolve-from": "4.0.0",
    "rimraf": "2.6.2",
    "semver": "5.5.0",
    "ts-loader": "4.4.1",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "uglifyjs-webpack-plugin": "1.2.6",
    "v8-compile-cache": "2.0.0",
    "webpack": "4.12.0",
    "webpack-node-externals": "1.7.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.15.3",
    "@oclif/test": "1.1.0",
    "@oclif/tslint": "1.1.2",
    "@types/babel__code-frame": "7.0.0",
    "@types/chai": "4.1.4",
    "@types/gulp": "4.0.5",
    "@types/gulp-babel": "6.1.29",
    "@types/gulp-filter": "3.0.32",
    "@types/gulp-sourcemaps": "0.0.32",
    "@types/lodash": "4.14.110",
    "@types/lodash-webpack-plugin": "0.11.3",
    "@types/merge2": "1.1.4",
    "@types/micromatch": "3.1.0",
    "@types/mocha": "5.2.3",
    "@types/node": "10.3.5",
    "@types/normalize-package-data": "2.4.0",
    "@types/rimraf": "2.0.2",
    "@types/semver": "5.5.0",
    "@types/uglifyjs-webpack-plugin": "1.1.0",
    "@types/webpack": "4.4.2",
    "@types/webpack-node-externals": "1.6.3",
    "chai": "4.1.2",
    "codecov": "3.0.2",
    "husky": "0.15.0-rc.13",
    "lint-staged": "7.2.0",
    "mocha": "5.2.0",
    "nyc": "12.0.2",
    "prettier": "1.13.5",
    "rxjs": "6.2.1",
    "rxjs-compat": "6.2.1",
    "ts-node": "7.0.0",
    "tslint": "5.10.0",
    "tslint-config-escapace": "1.2.0",
    "tslint-config-prettier": "1.13.0",
    "tslint-config-standard": "7.1.0",
    "tslint-microsoft-contrib": "5.0.3",
    "typescript": "2.9.2",
    "validate-commit": "3.4.0"
  },
  "engines": {
    "node": "10.5.0"
  },
  "files": [
    ".oclif.manifest.json",
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/escapace/recce",
  "keywords": [
    "TypeScript",
    "build"
  ],
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "recce",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "escapace/recce",
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write --parser typescript --trailing-comma none --no-semi --single-quote",
      "git add"
    ],
    "{src,test}/**/*.js": [
      "prettier --write --trailing-comma none --no-semi --single-quote",
      "git add"
    ],
    "**/*.json": [
      "prettier --write --parser json",
      "git add"
    ],
    "**/*.md": [
      "prettier --parser markdown --print-width 80",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": "lint-staged"
    }
  },
  "nyc": {
    "tempDirectory": "./node_modules/.cache/nyc_output",
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "test/**/*.test.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "scripts": {
    "build": "rimraf lib && tsc && oclif-dev manifest && oclif-dev readme",
    "test:travis": "npm run lint && npm run test",
    "typecheck": "tsc --noEmit",
    "lint": "tslint -p . -t stylish && tslint -p test -t stylish",
    "postpack": "rimraf .oclif.manifest.json",
    "prepack": "npm run build",
    "test": "tsc --noEmit -p test && mocha --forbid-only \"test/**/*.test.ts\"",
    "prerelease": "oclif-dev manifest && oclif-dev readme && git add README.md",
    "test:coverage": "nyc npm run test",
    "report-coverage": "npm run test:coverage && nyc report --reporter=text-lcov > coverage.lcov && codecov ; rimraf coverage.lcov"
  },
  "types": "lib/index.d.ts"
}
