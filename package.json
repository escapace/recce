{
  "name": "recce",
  "description": "Development tools for TypeScript libraries.",
  "version": "2.0.0",
  "author": "escapace <opensource@escapace.com>",
  "bin": {
    "recce": "./bin/run"
  },
  "bugs": "https://github.com/escapace/recce/issues",
  "dependencies": {
    "@babel/code-frame": "7.5.5",
    "@babel/core": "7.6.2",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/preset-env": "7.6.2",
    "@escapace/logger": "0.2.1",
    "@oclif/command": "1.5.19",
    "@oclif/config": "1.13.3",
    "@oclif/errors": "1.2.2",
    "@oclif/plugin-help": "2.2.1",
    "babel-loader": "8.0.6",
    "babel-plugin-annotate-pure-calls": "0.4.0",
    "babel-plugin-istanbul": "5.2.0",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-ramda": "2.0.0",
    "common-dir": "2.0.2",
    "eslint": "6.5.1",
    "find-npm-prefix": "1.0.2",
    "gzip-size": "5.1.1",
    "immer": "4.0.1",
    "istanbul-lib-coverage": "2.0.5",
    "istanbul-lib-report": "2.0.8",
    "istanbul-lib-source-maps": "3.0.6",
    "istanbul-reports": "2.2.6",
    "karma": "4.3.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.0",
    "karma-mocha": "1.3.0",
    "lodash": "4.17.15",
    "lodash-webpack-plugin": "0.11.5",
    "micromatch": "4.0.2",
    "mocha": "6.2.1",
    "normalize-package-data": "2.5.0",
    "ora": "4.0.2",
    "pkg-up": "3.1.0",
    "pretty-bytes": "5.3.0",
    "puppeteer": "1.20.0",
    "recursive-readdir": "2.2.2",
    "redux": "4.0.4",
    "reselect": "4.0.0",
    "resolve-from": "5.0.0",
    "rimraf": "3.0.0",
    "semver": "6.3.0",
    "source-map-loader": "0.2.4",
    "temp": "0.9.0",
    "terser-webpack-plugin": "2.1.2",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "tslib": "1.10.0",
    "typescript": "3.6.3",
    "v8-compile-cache": "2.1.0",
    "webpack": "4.41.0",
    "webpack-node-externals": "1.7.2"
  },
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@oclif/dev-cli": "1.22.2",
    "@oclif/test": "1.2.5",
    "@semantic-release/exec": "3.3.7",
    "@semantic-release/git": "7.0.16",
    "@types/temp": "0.8.34",
    "@types/babel__code-frame": "7.0.1",
    "@types/babel__core": "7.1.3",
    "@types/chai": "4.2.3",
    "@types/gzip-size": "5.1.1",
    "@types/istanbul-lib-coverage": "2.0.1",
    "@types/istanbul-lib-report": "1.1.1",
    "@types/istanbul-lib-source-maps": "1.2.2",
    "@types/istanbul-reports": "1.1.1",
    "@types/karma": "3.0.3",
    "@types/lodash": "4.14.141",
    "@types/lodash-webpack-plugin": "0.11.3",
    "@types/micromatch": "3.1.0",
    "@types/mocha": "5.2.7",
    "@types/node": "12.7.11",
    "@types/normalize-package-data": "2.4.0",
    "@types/ora": "3.2.0",
    "@types/pretty-bytes": "5.2.0",
    "@types/puppeteer": "1.20.1",
    "@types/recursive-readdir": "2.2.0",
    "@types/rimraf": "2.0.2",
    "@types/semver": "6.0.2",
    "@types/webpack": "4.39.2",
    "@types/webpack-node-externals": "1.6.3",
    "@typescript-eslint/eslint-plugin": "2.3.1",
    "@typescript-eslint/parser": "2.3.1",
    "chai": "4.2.0",
    "codecov": "3.6.1",
    "dir-compare": "1.7.3",
    "eslint-config-escapace": "2.0.1",
    "eslint-config-prettier": "6.3.0",
    "eslint-config-standard-with-typescript": "10.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsdoc": "15.9.2",
    "eslint-plugin-no-null": "1.0.2",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "husky": "3.0.8",
    "lint-staged": "9.4.1",
    "nyc": "14.1.1",
    "prettier": "1.18.2",
    "semantic-release": "15.13.24",
    "syncpack": "4.5.4",
    "ts-node": "8.4.1"
  },
  "engines": {
    "node": ">=10.16.0"
  },
  "files": [
    ".oclif.manifest.json",
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/escapace/recce",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && npm run typecheck && npm run test:coverage"
    }
  },
  "keywords": [
    "TypeScript",
    "build"
  ],
  "license": "MPL-2.0",
  "lint-staged": {
    "package.json": [
      "syncpack-format --source",
      "syncpack-set-semver-ranges --dev --source",
      "git add"
    ],
    "{src,test}/**/*.ts": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "{src,test}/**/*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "main": "lib/index.js",
  "nyc": {
    "cache": false,
    "tempDirectory": "./node_modules/.cache/nyc_output",
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "coverage/**",
      "packages/*/test/**",
      "test/**",
      "test{,-*}.ts",
      "**/*{.,-}{test,spec}.ts",
      "**/__tests__/**",
      "**/node_modules/**"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "recce",
    "plugins": []
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": "escapace/recce",
  "scripts": {
    "build": "rimraf lib && tsc && oclif-dev manifest",
    "lint": "eslint 'src/**/*.ts' 'test/**/*.ts'",
    "postpack": "rimraf .oclif.manifest.json",
    "prepack": "npm run build",
    "pretest": "./manage prepare-test-fixtures",
    "readme": "oclif-dev manifest && oclif-dev readme",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov ; rimraf coverage.lcov",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "test:coverage": "nyc npm run test",
    "test:travis": "npm run typecheck && npm run lint && npm run test:coverage",
    "typecheck": "tsc --noEmit && tsc --noEmit -p test"
  },
  "types": "lib/index.d.ts"
}
