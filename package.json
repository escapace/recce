{
  "name": "recce",
  "description": "Development tools for TypeScript libraries.",
  "version": "2.0.0",
  "author": "escapace <opensource@escapace.com>",
  "bin": {
    "recce": "./bin/run"
  },
  "bugs": "https://github.com/escapace/recce/issues",
  "dependencies": {
    "@babel/code-frame": "7.12.11",
    "@babel/core": "7.12.10",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.12.11",
    "@escapace/logger": "0.3.23",
    "@oclif/command": "1.8.0",
    "@oclif/config": "1.17.0",
    "@oclif/errors": "1.3.4",
    "@oclif/plugin-help": "3.2.1",
    "babel-loader": "8.2.2",
    "babel-plugin-annotate-pure-calls": "0.4.0",
    "babel-plugin-istanbul": "6.0.0",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-ramda": "2.0.0",
    "common-dir": "3.0.0",
    "find-npm-prefix": "1.0.2",
    "gzip-size": "6.0.0",
    "immer": "8.0.0",
    "istanbul-lib-coverage": "3.0.0",
    "istanbul-lib-report": "3.0.0",
    "istanbul-lib-source-maps": "4.0.0",
    "istanbul-reports": "3.0.2",
    "karma": "6.0.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-mocha": "2.0.1",
    "lodash": "4.17.20",
    "lodash-webpack-plugin": "0.11.6",
    "micromatch": "4.0.2",
    "mocha": "8.2.1",
    "normalize-package-data": "3.0.0",
    "ora": "5.2.0",
    "pkg-up": "3.1.0",
    "pretty-bytes": "5.5.0",
    "puppeteer": "5.5.0",
    "recursive-readdir": "2.2.2",
    "redux": "4.0.5",
    "reselect": "4.0.0",
    "resolve-from": "5.0.0",
    "rimraf": "3.0.2",
    "semver": "7.3.4",
    "source-map-loader": "1.1.3",
    "temp": "0.9.4",
    "terser-webpack-plugin": "4.2.0",
    "tsconfig-paths-webpack-plugin": "3.3.0",
    "tslib": "2.1.0",
    "typescript": "4.0.5",
    "v8-compile-cache": "2.2.0",
    "webpack": "5.25.0",
    "webpack-node-externals": "2.5.2"
  },
  "devDependencies": {
    "@commitlint/cli": "9.1.1",
    "@commitlint/config-conventional": "9.1.1",
    "@oclif/dev-cli": "1.22.2",
    "@oclif/test": "1.2.6",
    "@semantic-release/exec": "5.0.0",
    "@semantic-release/git": "9.0.0",
    "@types/babel__code-frame": "7.0.1",
    "@types/babel__core": "7.1.9",
    "@types/bluebird": "3.5.32",
    "@types/chai": "4.2.11",
    "@types/gzip-size": "5.1.1",
    "@types/istanbul-lib-coverage": "2.0.3",
    "@types/istanbul-lib-report": "3.0.0",
    "@types/istanbul-lib-source-maps": "4.0.1",
    "@types/istanbul-reports": "1.1.2",
    "@types/karma": "5.0.0",
    "@types/lodash": "4.14.157",
    "@types/lodash-webpack-plugin": "0.11.4",
    "@types/micromatch": "4.0.1",
    "@types/mocha": "8.0.0",
    "@types/node": "14.0.23",
    "@types/normalize-package-data": "2.4.0",
    "@types/ora": "3.2.0",
    "@types/pretty-bytes": "5.2.0",
    "@types/puppeteer": "3.0.1",
    "@types/recursive-readdir": "2.2.0",
    "@types/rimraf": "3.0.0",
    "@types/semver": "7.3.1",
    "@types/temp": "0.8.34",
    "@types/webpack": "4.41.21",
    "@typescript-eslint/eslint-plugin": "3.7.0",
    "@typescript-eslint/parser": "3.7.0",
    "chai": "4.2.0",
    "codecov": "3.7.1",
    "dir-compare": "2.3.0",
    "eslint": "7.6.0",
    "eslint-config-escapace": "3.1.2",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-standard-with-typescript": "18.0.2",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsdoc": "30.0.3",
    "eslint-plugin-no-null": "1.0.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "nyc": "15.1.0",
    "prettier": "2.0.5",
    "semantic-release": "17.1.1",
    "syncpack": "5.0.3",
    "ts-node": "8.10.2"
  },
  "engines": {
    "node": ">=10.16.0"
  },
  "files": [
    ".oclif.manifest.json",
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/escapace/recce",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && npm run typecheck && npm run test"
    }
  },
  "keywords": [
    "TypeScript",
    "build"
  ],
  "license": "MPL-2.0",
  "lint-staged": {
    "package.json": [
      "syncpack-format --source",
      "syncpack-set-semver-ranges --dev --source"
    ],
    "{src,test}/**/*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "{src,test}/**/*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "main": "lib/index.js",
  "nyc": {
    "cache": false,
    "tempDirectory": "./node_modules/.cache/nyc_output",
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "coverage/**",
      "packages/*/test/**",
      "test/**",
      "test{,-*}.ts",
      "**/*{.,-}{test,spec}.ts",
      "**/__tests__/**",
      "**/node_modules/**"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "recce",
    "plugins": []
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": "escapace/recce",
  "scripts": {
    "build": "rimraf lib && tsc && oclif-dev manifest",
    "lint": "eslint 'src/**/*.ts' 'test/**/*.ts'",
    "postpack": "rimraf .oclif.manifest.json",
    "prepack": "npm run build",
    "pretest": "./manage prepare-test-fixtures",
    "readme": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --config ./test/mocharc.json \"test/**/*.test.ts\"",
    "typecheck": "tsc --noEmit && tsc --noEmit -p test"
  },
  "types": "lib/index.d.ts"
}
